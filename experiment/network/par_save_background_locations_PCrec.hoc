proc par_save_background_locations_PCrec(){ 
	{ PC.barrier() } 
	strdef ext
	if ($4) {
	ext = "bin"
	} else {
	ext = "txt"
	}
	for src=0, nPCcells-1{
		{PC.barrier()}
		if (object_id(bgstimpidPC2PC[src]) != 0){
			str = ""
			sprint(str,"Background Stim locations src %d:",src)
			for ii=0,bgstimpidPC2PC[src].size()-1{
				sprint(str,"%s %f",str, bgstimpidPC2PC[src].x[ii])
			}
			print str
		}
	}
}